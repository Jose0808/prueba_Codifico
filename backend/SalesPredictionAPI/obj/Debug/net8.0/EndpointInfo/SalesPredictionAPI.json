            "format": "int32"
          },
          "shipperID": {
            "type": "integer",
            "format": "int32"
          },
          "shipName": {
            "type": "string",
            "nullable": true
          },
          "shipAddress": {
            "type": "string",
            "nullable": true
          },
          "shipCity": {
            "type": "string",
            "nullable": true
          },
          "shipCountry": {
            "type": "string",
            "nullable": true
          },
          "orderDate": {
            "type": "string",
            "format": "date-time"
          },
          "requiredDate": {
            "type": "string",
            "format": "date-time"
          },
          "shippedDate": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "freight": {
            "type": "number",
            "format": "double"
          },
          "productID": {
            "type": "integer",
            "format": "int32"
          },
          "unitPrice": {
            "type": "number",
            "format": "double"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "discount": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      }
    }
